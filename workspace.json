{
  "version": 2,
  "projects": {
    "operators-debug": {
      "root": "packages/operators/debug",
      "sourceRoot": "packages/operators/debug/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/operators/debug/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/operators/debug"],
          "options": {
            "jestConfig": "packages/operators/debug/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": { "defaultCollection": "@nrwl/workspace" }
}
